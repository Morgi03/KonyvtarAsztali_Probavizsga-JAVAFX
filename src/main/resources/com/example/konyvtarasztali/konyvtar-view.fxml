<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="600.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/18" fx:controller="com.example.konyvtarasztali.KonyvtarController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <Button fx:id="torlesGomb" contentDisplay="CENTER" mnemonicParsing="false" onAction="#torlesKatt" text="Button" />
   <TableView fx:id="KonyvTablazat" prefHeight="200.0" prefWidth="200.0" VBox.vgrow="ALWAYS">
     <columns>
       <TableColumn fx:id="cimOszlop" prefWidth="75.0" text="Cím" />
       <TableColumn fx:id="szerzoOszlop" prefWidth="75.0" text="Szerző" />
         <TableColumn fx:id="kiadasEve" prefWidth="75.0" text="Kiadás Éve" />
         <TableColumn fx:id="oldalszamOszlop" prefWidth="75.0" text="Oldalszám" />
     </columns>
   </TableView>
</VBox>
